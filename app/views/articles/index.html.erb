<h1>Articles#index</h1>
<p>Find me in app/views/articles/index.html.erb</p>

<%= link_to "Créer un article", articles_new_path %>
<%= @articles.each do |article| %>
<h2><%= article.title %></h2>
<p><%= article.content %></p>
<%= link_to "Accéder à l'article", articles_show_path(article) %>
<%= end %>
